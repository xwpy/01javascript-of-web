<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Document</title>
</head>
<body>
	<script type="text/javascript">
		//斐波那契数列
		//1、1、2、3、5、8、13、21、34……

		//输出前100项的斐波那契数列
		for(i = 1 ; i <= 100 ; i++){
			console.log(fib(i));
		}

		function fib(n){
			if(n == 1){
				return 1 ;
			}
			else if(n == 2){
				return 1 ;
			}
			else{
				return fib(n - 2) + fib(n - 1) ;
			}
		}
	</script>
</body>
</html>
