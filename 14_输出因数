<body>
	<script>
		// 【题目】用户输入输入一个数字，找到所有能除尽它的数。
		//【举例】用户输入48，那么控制台输出1、2、3、4、6、8、12、16、24、48
		var num = parseInt(prompt("please enter a numbe : "));
		for(i = 1; i<=num; i++){
			if(num%i == 0){
				console.log(i);}
		}
	</script>
</body>
