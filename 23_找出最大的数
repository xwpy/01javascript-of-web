<body>
	<script type="text/javascript">
		var a = parseInt(prompt("please enter the frist number:"));
		var b = parseInt(prompt("please enter the secondly number:"))
		bignum(a, b);
		function bignum(a, b){
			if(a > b){
				return a ;
			}
			else{
				return b ;
			}
		}
		
	</script>
